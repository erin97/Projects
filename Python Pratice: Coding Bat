//WarmUp
def monkey_trouble(a_smile, b_smile):
  if a_smile and b_smile:
    return True
  if not a_smile and not b_smile:
    return True
  return False

//Given two int values, return their sum. Unless the two values are the same, then return double their sum.
def sum_double(a, b):
  sum = a + b
  
  if a == b:
   sum = sum * 2
  return sum
    
